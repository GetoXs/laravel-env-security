{
    "name": "stechstudio/laravel-env-security",
    "description": "Securely manage .env files for different deployment environments",
    "type": "library",
    "require": {
        "aws/aws-sdk-php": "^3.0",
        "illuminate/console": "5.*",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "STS\\EnvSecurity\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "STS\\EnvSecurity\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\EnvSecurity\\EnvSecurityServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bubba",
            "email": "rob@stechstudio.com"
        },
        {
            "name": "Joseph Szobody",
            "email": "joseph@stechstudio.com"
        }
    ]
}
