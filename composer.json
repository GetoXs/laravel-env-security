{
    "name": "stechstudio/kmsdotenv",
    "description": "AWS KMS Encryption of .env files.",
    "type": "library",
    "require": {
        "aws/aws-sdk-php": "^3.61",
        "illuminate/console": "^5.6",
        "symfony/framework-bundle": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "STS\\Kms\\DotEnv\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "STS\\Kms\\DotEnv\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\Kms\\DotEnv\\ServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bubba",
            "email": "rob@stechstudio.com"
        }
    ]
}
